{
    field(DESC, "JogJ1 calc")
    field(INPA, "$(P):JOGJ1POS_CMD")
    field(INPB, "$(P):JOGJ1NEG_CMD")

#Guard against both jog buttons being pressed
    field(CALC,  "((A)|(B<<1))=3?0:((A)|(B<<1))")
    field(FLNK, "$(P):JOG_CALCOUT")
}

